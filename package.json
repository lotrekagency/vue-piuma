{
  "name": "vue-piuma",
  "version": "1.1.0",
  "description": "Vue component for Piuma Image optimizer",
  "repository": {
    "url": "https://github.com/Remeic/vue-piuma",
    "type": "git"
  },
  "main": "dist/vue-piuma.common.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "test-vue-app",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "Giulio Fagioli",
    "email": "fagioli.giulio@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "entry": "src/index.vue",
    "format": [
      "cjs",
      "umd"
    ],
    "plugins": [
      "vue"
    ],
    "vue": {
      "css": "dist/test.css"
    }
  },
  "devDependencies": {
    "bili": "^0.18.2",
    "chai": "^4.1.2",
    "poi": "^9.3.10",
    "rollup-plugin-vue": "^2.5.2",
    "test-vue-app": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.2"
  }
}
